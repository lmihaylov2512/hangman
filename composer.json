{
    "name": "lmihaylov/hangman",
    "description": "The hangman game that is implemented like web application.",
    "keywords": ["hangman", "multiplayer hangman", "open source hangman"],
    "homepage": "https://github.com/lmihaylov2512/hangman",
    "type": "project",
    "license": "MIT",
    "support": {
        "issues": "https://github.com/lmihaylov2512/hangman/issues",
        "source": "https://github.com/lmihaylov2512/hangman"
    },
    "minimum-stability": "stable",
    "require": {
        "php": ">=7.0.0",
        "yiisoft/yii2": "~2.0.6",
        "yiisoft/yii2-bootstrap": "~2.0.0",
        "yiisoft/yii2-swiftmailer": "~2.0.0",
        "bower-asset/morrisjs": "^0.5.1"
    },
    "require-dev": {
        "yiisoft/yii2-debug": "~2.0.0",
        "yiisoft/yii2-gii": "~2.0.0",
        "yiisoft/yii2-faker": "~2.0.0",

        "codeception/base": "^2.2.3",
        "codeception/verify": "~0.3.1"
    },
    "config": {
        "process-timeout": 1800,
        "fxp-asset":{
            "installer-paths": {
                "npm-asset-library": "vendor/npm",
                "bower-asset-library": "vendor/bower"
            }
        }
    }
}
